<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    android:orientation="horizontal" >

    <!-- Sets up ListView layout -->

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical" >

        <!-- Name Label -->

        <TextView
            android:id="@+id/name"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:paddingBottom="2dip"
            android:paddingTop="6dip"
            android:textColor="#21628d"
            android:textSize="20sp"
            android:textStyle="bold" />

        <!-- Address label -->

        <TextView
            android:id="@+id/address"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:paddingBottom="2dip"
            android:textSize="15sp"
            android:textColor="#acacac" >
        </TextView>

        <!-- Linear layout for city and state -->

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal" >

            <!-- Address Label -->

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="left"
                android:textColor="#5d5d5d"
                android:textSize="15sp"
                android:textStyle="bold" />

            <!-- City Label -->

            <TextView
                android:id="@+id/city"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="left"
                android:textColor="#acacac"
                android:textSize="15sp"
                android:textStyle="bold" >
            </TextView>

            <!-- Label for a comma -->

            <TextView
                android:id="@+id/comma"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="left"
                android:text="@string/comma"
                android:textColor="#acacac"
                android:textSize="15sp"
                android:textStyle="bold" />

            <!-- State Label -->

            <TextView
                android:id="@+id/state"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="left"
                android:textColor="#acacac"
                android:textSize="15sp"
                android:textStyle="bold" >
            </TextView>
        </LinearLayout>
    </LinearLayout>
    <!-- licenseInfo label -->

    <TextView
        android:id="@+id/licenseInfo"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:textSize="0sp"
        android:textColor="#acacac" >
    </TextView>
    <!-- Owner label -->

    <TextView
        android:id="@+id/ownerName"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:textSize="0sp"
        android:textColor="#acacac" >
    </TextView>
    <!-- Zipcode label -->

    <TextView
        android:id="@+id/zipCode"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:textSize="0sp"
        android:textColor="#acacac" >
    </TextView>
    <!-- Phone label -->

    <TextView
        android:id="@+id/phone"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:textSize="0sp"
        android:textColor="#acacac" >
    </TextView>
    
    <!-- Latitude label -->

    <TextView
        android:id="@+id/latitude"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:textSize="0sp"
        android:textColor="#acacac" >
    </TextView>
    
    <!-- Longitude label -->

    <TextView
        android:id="@+id/longitude"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:textSize="0sp"
        android:textColor="#acacac" >
    </TextView>
    
    <!-- Capacity label -->

    <TextView
        android:id="@+id/capacity"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:textSize="0sp"
        android:textColor="#acacac" >
    </TextView>
    <!-- OpenTime label -->

    <TextView
        android:id="@+id/openTime"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:textSize="0sp"
        android:textColor="#acacac" >
    </TextView>
    <!-- CloseTime label -->

    <TextView
        android:id="@+id/closeTime"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:textSize="0sp"
        android:textColor="#acacac" >
    </TextView>
    <!-- Specialist label -->

    <TextView
        android:id="@+id/specialist"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:textSize="0sp"
        android:textColor="#acacac" >
    </TextView>
    <!-- Specialist Phone label -->

    <TextView
        android:id="@+id/specialistPhone"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:textSize="0sp"
        android:textColor="#acacac" >
    </TextView>
    <!-- ABC Quality label -->

    <TextView
        android:id="@+id/qualityLevel"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:textSize="0sp"
        android:textColor="#acacac" >
    </TextView>

